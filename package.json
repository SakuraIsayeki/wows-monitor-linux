{
  "name": "@wows-monitor/app",
  "main": "app/main.js",
  "version": "1.1.0",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:web": "npm run build -- -c web",
    "ng:serve": "ng serve -c dev",
    "ng:serve:web": "ng serve -c dev-web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && npm run electron:build:e",
    "electron:build:e": "electron-builder build --windows",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "translate-extract": "ngx-translate-extract --i ./src --parent ./node_modules/@stewie/framework/assets/i18n/en-default.json --ignore ./src/assets/i18n/ignore.json --output ./src/assets/i18n/en.json --uiMessageImport @components/base.component,BaseComponent -f namespaced-json --sort --null-as-default-value true -c",
    "generate-client": "ng-openapi-gen && npm run generate-forms",
    "generate-forms": "node node_modules/@stewie/framework/scripts/generate.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "12.0.4",
    "@angular/compiler": "12.0.4",
    "@angular/compiler-cli": "12.0.4",
    "@angular/core": "12.0.4",
    "@angular/forms": "12.0.4",
    "@angular/language-service": "12.0.4",
    "@angular/platform-browser": "12.0.4",
    "@angular/platform-browser-dynamic": "12.0.4",
    "@angular/pwa": "^12.0.4",
    "@angular/router": "12.0.4",
    "@angular/service-worker": "12.0.4",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ngx-loading-bar/core": "~5.1.2",
    "@ngx-loading-bar/http-client": "^5.1.2",
    "@ngx-loading-bar/router": "~5.1.2",
    "@stewie/framework": "^1.0.305",
    "@stewie/meta": "^1.0.0",
    "@stewieoo/signalr": "^5.0.7",
    "@zxing/ngx-scanner": "~3.1.3",
    "device-uuid": "^1.0.4",
    "electron-log": "4.3.5",
    "electron-updater": "4.3.9",
    "electron-window-state": "5.0.3",
    "node-machine-id": "^1.1.12",
    "rxjs": "6.5.4",
    "skeleton-css": "^2.0.4",
    "tslib": "^2.3.0",
    "universal-analytics": "^0.4.23"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.0",
    "@angular-devkit/build-angular": "12.0.4",
    "@angular-devkit/schematics": "^12.0.4",
    "@angular/animations": "12.0.4",
    "@angular/cdk": "8.2.3",
    "@angular/cli": "^12.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "~1.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@stewieoo/ng-openapi-gen": "^0.17.2",
    "@stewieoo/ngx-translate-extract": "~7.0.6",
    "@types/node": "~15.12.2",
    "codelyzer": "^6.0.0",
    "conventional-changelog-cli": "2.0.25",
    "core-js": "3.1.3",
    "electron": "~13.1.2",
    "electron-builder": "^22.11.7",
    "electron-reload": "1.5.0",
    "fast-xml-parser": "3.14.0",
    "npm-run-all": "~4.1.5",
    "path": "^0.12.7",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^12.0.0",
    "semver": "6.3.0",
    "showdown": "^1.9.1",
    "ts-node": "8.2.0",
    "tslint": "~6.1.0",
    "typescript": "4.2.4",
    "uuid": "^3.3.3",
    "wait-on": "3.3.0",
    "webdriver-manager": "12.1.4",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "chrome 83"
  ]
}
